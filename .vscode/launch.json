            "name": ".NET Core Launch (Admiministrator)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-Administracion",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/app-administracion/administracion/bin/Debug/net6.0/administracion.dll",
            "args": [],
            "cwd": "${workspaceFolder}/app-administracion/administracion",
            "stopAtEntry": false,
            // Enable launching a web browser when ASP.NET Core starts. For more information: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
